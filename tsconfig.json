{
	"compilerOptions": {
		"target": "es2015",
		"moduleResolution": "node",
		"module": "commonjs",
		"typeRoots": [
			"node_modules/@types",
			"src/@types"
		],
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"jsx": "react"
	},
	"exclude": [
		"node_modules",
	]
}